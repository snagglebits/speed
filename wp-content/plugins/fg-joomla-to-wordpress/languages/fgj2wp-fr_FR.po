msgstr "SEO : garde les URLs OpenSEF"

#: admin_build_page.tpl.php:177
msgid "Joomla views counts migration"
msgstr "migration des compteurs de vues de Joomla"

#: admin_build_page.tpl.php:178
msgid "Joomla 1.0 compatible"
msgstr "compatible Joomla 1.0"

#: admin_build_page.tpl.php:179
