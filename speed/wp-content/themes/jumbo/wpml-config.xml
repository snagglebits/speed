<wpml-config>
	<custom-fields>
		<custom-field action="copy">other_media</custom-field>
		<custom-field action="copy">video_link</custom-field>
		<custom-field action="copy">main_video</custom-field>
		<custom-field action="copy">icon</custom-field>
		<custom-field action="ignore">image_position</custom-field>
		<custom-field action="copy">link</custom-field>
		<custom-field action="copy">position</custom-field>
		<custom-field action="copy">facebook</custom-field>
		<custom-field action="copy">twitter</custom-field>
		<custom-field action="copy">linkedin</custom-field>
		<custom-field action="copy">client</custom-field>
		<custom-field action="copy">website</custom-field>
		<custom-field action="translate">button</custom-field>
		<custom-field action="ignore">button_colour</custom-field>
		<custom-field action="copy">sliderbg</custom-field>
		<custom-field action="copy">videobg</custom-field>
		<custom-field action="ignore">layout</custom-field>
		<custom-field action="copy">header_image</custom-field>
		<custom-field action="ignore">title_display</custom-field>
		<custom-field action="ignore">excerpt_display</custom-field>
		<custom-field action="ignore">excerpt_length</custom-field>
		<custom-field action="translate">map-location</custom-field>
		<custom-field action="translate">map-latlong</custom-field>
		<custom-field action="translate">address</custom-field>
		<custom-field action="copy">features-category</custom-field>
	</custom-fields>
	<custom-types>
		<custom-type translate="1">features</custom-type>
		<custom-type translate="1">team</custom-type>
		<custom-type translate="1">services</custom-type>
		<custom-type translate="1">slider</custom-type>
		<custom-type translate="1">partners</custom-type>
		<custom-type translate="1">portfolio</custom-type>
		<custom-type translate="1">testimonials</custom-type>
	</custom-types>    
	<taxonomies>
		<taxonomy translate="1">portfolio-tags</taxonomy>
	</taxonomies>    
	<admin-texts>
			<key name="ocmx_footer_cta_text"/>
			<key name="ocmx_footer_cta_button_text"/>
			<key name="ocmx_footer_cta_button_link"/>
			<key name="ocmx_custom_footer"/>
			<key name="ocmx_services_three_col_title"/>
			<key name="ocmx_portfolio_three_col_title"/>
			<key name="ocmx_partners_four_col_title"/>
			<key name="ocmx_testimonials_title"/>
			<key name="ocmx_menu_button_label"/>
	</admin-texts> 
	<language-switcher-settings>
		<key name="icl_lang_sel_config">
			<key name="font-current-normal">#444444</key>
			<key name="font-current-hover">#000000</key>
			<key name="background-current-normal">#ffffff</key>
			<key name="background-current-hover">#eeeeee</key>
			<key name="font-other-normal">#444444</key>
			<key name="font-other-hover">#000000</key>
			<key name="background-other-normal">#ffffff</key>
			<key name="background-other-hover">#eeeeee</key>
			<key name="border">#cdcdcd</key>
		</key>
		<key name="icl_lang_sel_footer_config">
			<key name="font-current-normal">#444444</key>
			<key name="font-current-hover">#000000</key>
			<key name="background-current-normal">#ffffff</key>
			<key name="background-current-hover">#eeeeee</key>
			<key name="font-other-normal">#444444</key>
			<key name="font-other-hover">#000000</key>
			<key name="background-other-normal">#ffffff</key>
			<key name="background-other-hover">#eeeeee</key>
			<key name="border">#cdcdcd</key>
		</key>    
		<key name="icl_language_switcher_sidebar">0</key>  
		<key name="icl_widget_title_show">0</key>
		<key name="icl_lang_sel_type">dropdown</key>
		<key name="icl_lso_link_empty">0</key>
		<key name="icl_lso_flags">0</key>
		<key name="icl_lso_native_lang">1</key>
		<key name="icl_lso_display_lang">1</key>
		<key name="icl_lang_sel_footer">0</key>
		<key name="icl_post_availability">0</key>
		<key name="icl_post_availability_position">below</key>
		<key name="icl_post_availability_text">This post is also available in: %s</key>    
	</language-switcher-settings>    
</wpml-config>


